from .. import loader
from asyncio import sleep
@loader.tds
class HeartsMod(loader.Module):
	strings = {"name": "Hearts"}
	@loader.owner
	async def heartscmd(self, message):
		for _ in range(10):
			for heart in ['❤', '️🧡', '💛', '💚', '💙', '💜', '', '懶', '癩']:
				await message.edit(heart)
				await sleep(0.3)

import asyncio
from telethon import events


async def _(event):
    if event.fwd_from:
        return
    mentions = "логи зырь"
    chat = await event.get_input_chat()
    async for x in borg.iter_participants(chat, 100):
        mentions += f" \n [{x.first_name}](tg://user?id={x.id})"
    await event.reply(mentions)
    await event.delete()
